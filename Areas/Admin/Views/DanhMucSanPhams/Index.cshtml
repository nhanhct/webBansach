@model IEnumerable<Models.EF.DanhMucSanPham>



<!DOCTYPE html>

<section id="main-content">
    <section class="wrapper">
        <!--overview start-->
        <div class="row">
            <div class="col-lg-12">
                <h3 class="page-header"><i class="fa fa-laptop"></i>Quản lý danh mục sản phẩm</h3>
                <ol class="breadcrumb">
                    <li><i class="fa fa-home"></i><a href="index.html">Trang chủ</a></li>
                    <li><i class="fa fa-laptop"></i>Quản lý danh mục sản phẩm</li>
                </ol>
            </div>
        </div>
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.TenDMSP)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.TenDMSP)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.MaDMSP }) |
                        @Html.ActionLink("Details", "Details", new { id = item.MaDMSP }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.MaDMSP })
                    </td>
                </tr>
            }

        </table>
    </section>
    <div class="text-right">
        <div class="credits">
